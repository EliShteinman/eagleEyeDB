# The username for connecting to the MySQL database
DATABASE_USER="your_username"

# The password for the database connection
DATABASE_PASSWORD="your_password"

# The database host – usually "localhost" for local development
DATABASE_HOST="localhost"

# The name of the database that contains the agents table
DATABASE_NAME="eagleEyeDB"

# The MySQL port – default is 3306
DATABASE_PORT=3306

# The name of the table where agent data is stored
DATABASE_TABLE="agents"